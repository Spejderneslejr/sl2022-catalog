{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "watch:tailwind": "tailwindcss -i src/tailwind.css -o public/build/index.css --watch",
    "build:tailwind": "cross-env NODE_ENV=production postcss src/tailwind.css -o public/build/index.css",
    "build": "npm run  build:tailwind && rollup -c",
    "dev": "concurrently \"rollup -c -w\" \"npm run watch:tailwind\"",
    "start": "sirv public --cors --no-clear --port 4000",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write \"src/**/*.{ts,svelte}\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tailwindcss/typography": "^0.5.2",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "autoprefixer": "^10.4",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "daisyui": "^2.13.6",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "postcss": "^8.4.4",
    "postcss-cli": "^9.0.2",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.6",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "rollup": "^2.61.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-workbox": "^6.2.0",
    "sirv-cli": "^2.0.1",
    "svelte": "^3.46.2",
    "svelte-check": "^2.2.10",
    "svelte-preprocess": "^4.10.0",
    "tailwindcss": "^3.0.1",
    "tslib": "^2.3",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@arcgis/core": "^4.23.6",
    "dayjs": "^1.11.2",
    "fuse.js": "^6.5.3",
    "sswr": "^1.7.0",
    "svelte-spa-router": "^3.2.0"
  }
}
